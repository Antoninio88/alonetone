var soundIsReady = true;

soundManager.setup({
  debugMode: true,
  flashVersion: 9,
  useHTML5Audio: true, /* for acl, baby */
  consoleOnly: true,
  html5PollingInterval: 50,
  url: '<%= asset_path('soundmanager2_flash9.swf')%>'
});

if (navigator.userAgent.indexOf('Safari') != -1 && navigator.userAgent.indexOf('Chrome') == -1)
  soundManager.setup({ preferFlash: true });
