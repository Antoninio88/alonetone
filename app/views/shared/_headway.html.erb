<script defer src="https://cdn.headwayapp.co/widget.js"></script>
<script>
  // scripts marked defer will execute after domInteractive fires
  // After that, onload will fire and we'll excute the following code
  window.addEventListener("turbolinks:load", function() {
    // @see https://docs.headwayapp.co/widget for more configuration options.
    const config = {
      selector: ".headway", // CSS selector where to inject the badge
      account:  "yEwwZ7"
    }
    Headway.init(config)
  })

</script>